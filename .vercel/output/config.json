{
	"version": 3,
	"routes": [
		{
			"src": "^/(.*)/$",
			"headers": {
				"Location": "/$1"
			},
			"status": 308
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)$",
			"dest": "_render"
		},
		{
			"src": "^/_image$",
			"dest": "_render"
		},
		{
			"src": "^/_actions(?:/(.*?))?$",
			"dest": "_render"
		},
		{
			"src": "^/404$",
			"dest": "_render"
		},
		{
			"src": "^/consultation$",
			"dest": "_render"
		},
		{
			"src": "^/guide$",
			"dest": "_render"
		},
		{
			"src": "^/join$",
			"dest": "_render"
		},
		{
			"src": "^/math-nigate$",
			"dest": "_render"
		},
		{
			"src": "^/rikei$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	],
	"images": {
		"sizes": [
			640,
			750,
			828,
			1080,
			1200,
			1920,
			2048,
			3840
		],
		"domains": [
			"cdn.pixabay.com",
			"images.unsplash.com",
			"placehold.co",
			"lh3.googleusercontent.com"
		],
		"remotePatterns": []
	}
}